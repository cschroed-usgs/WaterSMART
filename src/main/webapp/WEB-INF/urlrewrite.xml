<?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite use-query-string="true">
	<rule>
		<from>^/proxy/?\?url=(.*)$</from>
		<to type="proxy">${unescape:$1}</to>
	</rule>
	<rule>
		<from>^/custom-sld.xml(.*)$</from>
		<to>/customstyle.jsp$1</to>
	</rule>
	<outbound-rule>
		<from>^/customstyle.jsp(.*)$</from>
		<to>/custom-sld.xml$1</to>
	</outbound-rule>
</urlrewrite>
